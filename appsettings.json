{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "CeiApiDbConnection": {
    "ConnectionString": "env:CeiApiDbConnection__ConnectionString",
    "DatabaseName": "cei_api"
  },
  "CeiApiDbCollection": {
    "CursoCollectionName": "cursos",
    "EstudianteCollectionName": "estudiantes",
    "MateriaCollectionName": "materias"
  },
  "CeiApiKey": {
    "Current": "user-secrets:CeiApiKey:Current|env:CeiApiKey__Current",
    "Academica": "env:CeiApiKey__Academica",
    "Encuestas": "env:CeiApiKey__Encuestas"
  }
}